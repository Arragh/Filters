@{ Layout = null; }

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Filters</title>
    <link rel="stylesheet" href="~/dist/bootstrap/css/bootstrap.css" />
</head>
<body class="m-1 p-1">
    @if (Model is string)
    {
        @Model
    }
    else if (Model is Dictionary<string, string>)
    {
        var dict = Model as Dictionary<string, string>;
        <table class="table table-sm table-bordered table-striped">
            <thead>
                <tr>
                    <th>
                        Name
                    </th>
                    <th>
                        Value
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var kvp in dict)
                {
                    <tr>
                        <td>
                            @kvp.Key
                        </td>
                        <td>
                            @kvp.Value
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    }
</body>
</html>